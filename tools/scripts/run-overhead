#!/usr/bin/ruby
# -*- coding: utf-8 -*-

$devNull = File.open(File::NULL, "w")

def runBenchmark args
    STDERR.puts args.join(" ")
    system args.join(" ")
end

class Test
    def initialize(name)
        @name = name
    end

    def execute
        args = @args.collect {|arg| arg.gsub("\n", "") }
        runBenchmark(args)
    end
end

class CUDATest < Test
    def initialize(name, *args)
        super(name)
        @args = args
    end
end

class KernelSplitTest < Test
    def initialize(name, *args)
        super(name)
        @args = args
    end
end

class GLoopTest < Test
    def initialize(name, *args)
        super(name)
        @args = args
    end
end

